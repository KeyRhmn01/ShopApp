<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_50cdp"

    >

    <TextView
        android:id="@+id/text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10cdp"
        android:layout_marginTop="@dimen/_10cdp"
        android:layout_marginEnd="@dimen/_10cdp"
        android:layout_marginBottom="@dimen/_10cdp"
        android:gravity="right"
        android:layout_centerVertical="true"
        android:text="list"
        android:textColor="@color/black"
        android:textSize="@dimen/_20cdp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="@color/black"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="@color/black"
        android:layout_alignParentBottom="true"

        />

    <View
        android:layout_width="2px"
        android:layout_height="match_parent"
        android:background="@color/black"
        />

    <View
        android:layout_width="2px"
        android:layout_height="match_parent"
        android:background="@color/black"
        android:layout_alignParentRight="true"

        />




</RelativeLayout>